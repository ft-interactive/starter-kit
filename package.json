{
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          },
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-knobs": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.0-beta.4",
    "chai": "^4.1.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.10",
    "csv-loader": "^3.0.2",
    "d3-time-format": "^2.2.3",
    "dotenv": "^6.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mdx": "^1.6.5",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "extract-loader": "^2.0.1",
    "file-loader": "^1.1.11",
    "generate-json-webpack-plugin": "^0.3.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^7.0.0",
    "imagemin-webpack-plugin": "^2.2.0",
    "jsdom": "^11.11.0",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^8.1.3",
    "pa11y": "^5.3.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.0.6",
    "postcss-preset-env": "^6.6.0",
    "postcss-scss": "^1.0.2",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "raw-loader": "^0.5.1",
    "react-is": "^16.13.1",
    "sass": "^1.24.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5.3.x"
  },
  "optionalDependencies": {
    "@financial-times/g-deploy": "^3.0.8",
    "@financial-times/vvc": "github:financial-times/visual-vocabulary-components#prerelease",
    "react-snap": "^1.23.0"
  },
  "private": true,
  "scripts": {
    "build": "rm -rf dist && webpack -p --env=production",
    "postbuild": "react-snap",
    "a11y": "pa11y $(g-deploy --get-commit-url)",
    "a11y:preview": "pa11y $(g-deploy --preview --get-commit-url)",
    "a11y:local": "pa11y localhost:8080",
    "deploy": "g-deploy --assets-prefix=https://ig.ft.com/v2/__assets/",
    "test": "mocha ./test/**/*.spec.js",
    "start": "webpack-dev-server -r dotenv/config",
    "lint": "eslint app/**/*.js test/**/*.js",
    "lint:fix": "eslint --fix app/**/*.js test/**/*.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "@financial-times/g-components": "^7.3.0",
    "@financial-times/o-colors": "^6.4.0",
    "@financial-times/o-grid": "^6.1.0",
    "@hot-loader/react-dom": "^16.11.0",
    "core-js": "^3.6.4",
    "prop-types": "^15.6.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet-async": "0.1.0-alpha",
    "react-hot-loader": "^4.12.18",
    "react-markdown": "^3.4.1"
  },
  "reactSnap": {
    "source": "dist"
  }
}
